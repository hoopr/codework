{
  "name": "codework",
  "version": "1.0.0-beta.2",
  "description": "Daily programming challenge turns into an interactive programming reference guide.",
  "bugs": {
    "url": "https://github.com/hoopr/codework/issues"
  },
  "contributors": [
    "Marie Hooper <marie.ashtari@gmail.com> (https://mariehooper.me)",
    "Patrick Hooper <patrick.d.hooper@gmail.com> (https://patrickhooper.me)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hoopr/codework"
  },
  "scripts": {
    "build": "react-scripts build",
    "postbuild": "node 'scripts/createRedirects.js'",
    "deploy": "npm-run-all lint build",
    "eject": "react-scripts eject",
    "lint": "npm-run-all --parallel 'lint:*'",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint --ignore-pattern '!.*' --ignore-pattern '/build/' --ignore-pattern '/node_modules/' --ignore-pattern '/src/utils/registerServiceWorker.js' .",
    "lint:sc": "cross-env STYLELINT_ENV=sc stylelint 'src/**/*.js'",
    "prepush": "npm run deploy",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "date-fns": "^1.28.5",
    "firebase": "^4.3.1",
    "marked": "^0.3.6",
    "normalize.css": "^7.0.0",
    "prismjs": "^1.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-onclickoutside": "^6.5.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.13",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^0.6.0",
    "stylelint-processor-styled-components": "^0.4.0",
    "typescript": "^2.5.2"
  },
  "private": true,
  "proxy": {
    "/api/codewars": {
      "changeOrigin": true,
      "pathRewrite": {
        "^/api/codewars": ""
      },
      "target": "https://www.codewars.com/api/v1"
    }
  }
}
